{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "build command",
			"command": "${workspaceFolder}/docker.sh",
			"args": [
				"build"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
                "kind": "build",
                "isDefault": true
            }
		}
	]
}